#include<stdio.h>
#include<conio.h>
int main()
{ 
int a,b,c,d,i;
printf("enter the lower value of sequence ");
scanf("%d",&a);
printf("enter the upper value of sequence ");
scanf("%d",&b);
printf("enter the difference value of sequence ");
scanf("%d",&c);
for(i=a;i<=b;i++)
{
printf(" %d \t",d);
c=d+c;
}
printf(" the size of variable is %d bytes", sizeof(d));
    return 0;
    getch();
}
